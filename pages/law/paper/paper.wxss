.paper-panel {
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
}

.paper-wrap {
  flex: 1;
  box-sizing: border-box;
  background: #fff;
}

.movable-area {
  height: 100%;
  width: 100%;
  background-color: #fff;
  overflow: hidden;
}

.paper {
  box-sizing: border-box;
  width: 1500rpx;
  height: auto;
  background: #fff;
  box-shadow: 0 0 20px 0 rgba(172, 173, 177, 0.6);
  font-size: 12px;
  color: #000;
  padding: 100rpx 150rpx;
}

.paper view {
  margin-bottom: 10px;
}

.paper text {
  border-bottom: 1px solid #000;
  display: inline-block;
  min-width: 60rpx;
}

.paper-checkbox {
  border: none !important;
  min-width: auto !important;
  padding: 0 !important;
}

.paper-checkbox::first-letter {
  font-size: 16px;
}

.paper-title {
  text-align: center;
  font-size: 20px;
  font-weight: bold;
  height: 30px;
  line-height: 30px;
}

.paper-flex1 {
  flex: 1;
}

.paper-flex2 {
  flex: 2;
}

.paper-flex3 {
  flex: 3;
}

.paper-flex4 {
  flex: 4;
}

.paper-flex5 {
  flex: 5;
}

.paper-flex-end {
  display: flex;
  justify-content: flex-end;
}

.paper-flex-between {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  flex-wrap: wrap;
}

.paper-flex-align-center {
  display: flex;
  align-items: center;
}

.paper-margin-bottom {
  margin-bottom: 35px !important;
}

.paper-margin-bottom0 {
  margin-bottom: 0 !important;
}

.paper-margin-left {
  margin-left: 10px !important;
}

.paper-padding-top0 {
  padding-top: 0 !important;
}

.paper-height20 {
  height: 20px;
}

.paper-padding {
  padding: 0 0 0 20px;
}

.paper-img {
  margin: 0 !important;
  border-bottom: 1px solid #000;
  display: flex;
  align-items: center;
  height: 40px;
}

.paper-img image {
  width: 100%;
  height: 100%;
}

.paper-btn {
  position: relative;
  z-index: 999;
  height: 120rpx;
  display: flex;
  justify-content: space-evenly;
  align-items: center;
  box-shadow: 0px 0px 10px 0px rgba(216, 211, 211, 0.6);
}

.paper-btn view {
  width: 320rpx;
}

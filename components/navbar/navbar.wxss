/* components/navbar/navbar.wxss */
@import "/iconfont.wxss";

.nav-wrap {
  position: fixed;
  width: 100%;
  height: 44px;
  top: 0;
  z-index: 11111;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  overflow: hidden;
}
.nav-back {
  position: absolute;
  left: 24rpx;
  width: 64rpx;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
}
.nav-back icon {
  font-size: 55rpx;
}
.nav-title {
  font-size: 36rpx;
  color: #333
}